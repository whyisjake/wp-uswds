{
  "name": "wp-uswds",
  "version": "1.0.0",
  "description": "WordPress Gutenberg blocks and theme based on the U.S. Web Design System (USWDS)",
  "main": "index.js",
  "scripts": {
    "install:assets": "node scripts/install-uswds-assets.js",
    "generate:blocks": "node scripts/generate-blocks.js",
    "deploy": "node scripts/deploy.js",
    "nvm:use": "nvm use",
    "preinstall": "node scripts/check-node-version.js",
    "verify:setup": "node scripts/verify-setup.js",
    "build:theme": "cd packages/uswds-theme && npm run build",
    "dev:theme": "cd packages/uswds-theme && npm run start"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "blocks",
    "uswds",
    "government",
    "accessibility",
    "theme"
  ],
  "author": "Jake Johnson <jake@jjj.blog> (https://jjj.blog)",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@uswds/uswds": "^3.13.0",
    "@wordpress/scripts": "^27.0.0",
    "archiver": "^5.3.2",
    "semver": "^7.5.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
